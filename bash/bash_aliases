# Aliases for bash

alias utrm='cd ~/code/utrm/src && python3 utrm.py'
alias sympy='cd ~/preferences/scripts && python3 -i sympy_session.py && cd'
alias conv='cd ~/preferences/scripts/unitconverter && python3 unitconverter.py'
alias ode='cd ~/preferences/scripts/odesolver && python3 odesolver.py'
alias linec='python3 ~/preferences/scripts/linecounter.py'
